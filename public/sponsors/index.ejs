<h1>Sponsors</h1>

<ul>
<% for (var sponsor in public.sponsors._data) { %>
<li>
    <img src="<%= public.sponsors._data[sponsor].image %>" width="100">
    <h2><a href="<%= public.sponsors._data[sponsor].url %>"><%= public.sponsors._data[sponsor].name %></a></h2>
    <p><%= public.sponsors._data[sponsor].blurb %></p>
    <p><%= public.sponsors._data[sponsor].promo %></p>
</li>
<% } %>
</ul>